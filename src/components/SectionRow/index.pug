      a.not-link.button.button--black.button--rounded.section-row__button-start
        span.button__text начать проект

mixin SectionRowMiddle
  .section-row.section-row--middle.section-row--border-bottom&attributes(attributes)
    .section-row__inner.container
      +HeaderTypes_sites.section-row__types
      +HeaderTypes_services.section-row__types

mixin SectionRowBottom
  .section-row.section-row--bottom&attributes(attributes)
    .section-row__inner.container
      p.section-row__copyright © 2021 Webster STD
      a.section-row__politic Политика конфиденциальности
      +Socials('true').section-row__socials
